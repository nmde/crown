{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off"
    }
}
